<!DOCTYPE html>
<html data-ng-app="VanzShopApp">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Vanz Shop</title>
    <!-- TODO: Need to bundle -->
    <link rel="stylesheet" href="~/Scripts/bower_components/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Scripts/bower_components/owl.carousel/dist/assets/owl.carousel.min.css" />
    <link rel="stylesheet" href="~/Scripts/bower_components/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/Scripts/bower_components/footable/css/footable.core.min.css" />
    <link rel="stylesheet" href="~/Scripts/bower_components/Smoothproducts/css/smoothproducts.css" />
    <link rel="stylesheet" href="~/Content/css/skin.css" />
    <link rel="stylesheet" href="~/Content/css/Site.css" />
</head>
<body>
    @RenderBody();
    <header ng-controller="indexViewModel">
        <div id="top"></div>
        <!-- Modal Login start -->
        <div class="modal signUpContent fade" id="ModalLogin" tabindex="-1" role="dialog">
            <form name="loginForm" novalidate>
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true"> &times; </button>
                            <h3 class="modal-title-site text-center"> Login to VANZ SHOP </h3>
                        </div>
                        <div class="modal-body">
                            <div class="form-group login-username has-feedback" show-errors="{ trigger: 'keypress' }">
                                <div>
                                    <input required name="loginUsername" id="loginUsername" ng-model="loginFormFields.username" class="form-control input" size="20" placeholder="Enter Username" type="text">
                                </div>

                                <span class="glyphicon glyphicon-remove form-control-feedback" ng-hide="!loginForm.loginUsername.$invalid || !loginForm.loginUsername.$dirty" aria-hidden="true"></span>
                                <span class="glyphicon glyphicon-ok form-control-feedback" ng-hide="loginForm.loginUsername.$invalid || !loginForm.loginUsername.$dirty" aria-hidden="true"></span>

                                <div ng-show="loginForm.loginUsername.$invalid && loginForm.loginUsername.$dirty" ng-messages="loginForm.loginUsername.$error">
                                    <p ng-message="required" class="help-block">{{ constantService.messages.CUSTOM_MESSAGE('Username', 'required') }}</p>
                                </div>
                            </div>
                            <div class="form-group login-password has-feedback" show-errors="{ trigger: 'keypress' }">
                                <div>
                                    <input required name="loginPassword" id="loginPassword" ng-model="loginFormFields.password" class="form-control input" size="20" placeholder="Password" type="password">
                                </div>

                                <span class="glyphicon glyphicon-remove form-control-feedback" ng-hide="!loginForm.loginPassword.$invalid || !loginForm.loginPassword.$dirty" aria-hidden="true"></span>
                                <span class="glyphicon glyphicon-ok form-control-feedback" ng-hide="loginForm.loginPassword.$invalid || !loginForm.loginPassword.$dirty" aria-hidden="true"></span>

                                <div ng-show="loginForm.loginPassword.$invalid && loginForm.loginPassword.$dirty" ng-messages="loginForm.loginPassword.$error">
                                    <p ng-message="required" class="help-block">{{ constantService.messages.CUSTOM_MESSAGE('Password', 'required') }}</p>
                                </div>
                            </div>
                            <div>
                                <div>
                                    <input name="submit" ng-disabled="loginForm.$invalid" data-dismiss="modal" class="btn  btn-block btn-lg btn-primary" value="LOGIN" type="submit" ng-click="loginUser(loginFormFields.username, loginFormFields.password)">
                                </div>
                            </div>
                            <!--userForm-->
                        </div>
                        <div class="modal-footer">
                            <p class="text-center">
                                Not here before? <a data-toggle="modal" data-dismiss="modal" href="#ModalSignup"> Sign Up. </a>
                            </p>
                        </div>
                    </div>
                    <!-- /.modal-content -->

                </div>
            </form>
            <!-- /.modal-dialog -->

        </div>
        <!-- /.Modal Login -->
        <!-- Modal Signup start -->
        <div class="modal signUpContent fade" id="ModalSignup" tabindex="-1" role="dialog">
            <form name="signupForm" novalidate>
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true"> &times; </button>
                            <h3 class="modal-title-site text-center"> REGISTER </h3>
                        </div>
                        <div class="modal-body">
                            <div class="form-group reg-username has-feedback" show-errors="{ trigger: 'keypress' }">
                                <div>
                                    <input required name="signupUsername" ng-model="signupFormFields.username" class="form-control input" size="20" placeholder="Enter Username" type="text">
                                </div>

                                <span class="glyphicon glyphicon-remove form-control-feedback" ng-hide="!signupForm.signupUsername.$invalid || !signupForm.signupUsername.$dirty" aria-hidden="true"></span>
                                <span class="glyphicon glyphicon-ok form-control-feedback" ng-hide="signupForm.signupUsername.$invalid || !signupForm.signupUsername.$dirty" aria-hidden="true"></span>

                                <div ng-show="signupForm.signupUsername.$invalid && signupForm.signupUsername.$dirty" ng-messages="signupForm.signupUsername.$error">
                                    <p ng-message="required" class="help-block">{{ constantService.messages.CUSTOM_MESSAGE('Username', 'required') }}</p>
                                </div>
                            </div>
                            <div class="form-group reg-email has-feedback" show-errors="{ trigger: 'keypress' }">
                                <div>
                                    <input required name="signupEmail" ng-model="signupFormFields.email" class="form-control input" size="20" placeholder="Enter Email" type="email">
                                </div>

                                <span class="glyphicon glyphicon-remove form-control-feedback" ng-hide="!signupForm.signupEmail.$invalid || !signupForm.signupEmail.$dirty" aria-hidden="true"></span>
                                <span class="glyphicon glyphicon-ok form-control-feedback" ng-hide="signupForm.signupEmail.$invalid || !signupForm.signupEmail.$dirty" aria-hidden="true"></span>

                                <div ng-show="signupForm.signupEmail.$invalid && signupForm.signupEmail.$dirty" ng-messages="signupForm.signupEmail.$error">
                                    <p ng-message="required" class="help-block">{{ constantService.messages.CUSTOM_MESSAGE('Email', 'required') }}</p>
                                </div>
                            </div>
                            <div class="form-group reg-password has-feedback" show-errors="{ trigger: 'keypress' }">
                                <div>
                                    <input required name="signupPassword" ng-model="signupFormFields.password" class="form-control input" size="20" placeholder="Password" type="password">
                                </div>

                                <span class="glyphicon glyphicon-remove form-control-feedback" ng-hide="!signupForm.signupPassword.$invalid || !signupForm.signupPassword.$dirty" aria-hidden="true"></span>
                                <span class="glyphicon glyphicon-ok form-control-feedback" ng-hide="signupForm.signupPassword.$invalid || !signupForm.signupPassword.$dirty" aria-hidden="true"></span>

                                <div ng-show="signupForm.signupPassword.$invalid && signupForm.signupUsername.$dirty" ng-messages="signupForm.signupPassword.$error">
                                    <p ng-message="required" class="help-block">{{ constantService.messages.CUSTOM_MESSAGE('Password', 'required') }}</p>
                                </div>
                            </div>
                            <div>
                                <div>
                                    <input name="submit" ng-disabled="signUpForm.$invalid" data-dismiss="modal" class="btn  btn-block btn-lg btn-primary" value="REGISTER" type="submit" ng-click="registerUser(signupFormFields)">
                                </div>
                            </div>
                            <!--userForm-->

                        </div>
                        <div class="modal-footer">
                            <p class="text-center"> Already member? <a data-toggle="modal" data-dismiss="modal" href="#ModalLogin"> Sign in </a> </p>
                        </div>
                    </div>
                    <!-- /.modal-content -->

                </div>
            </form>
            <!-- /.modal-dialog -->

        </div>
        <!-- /.ModalSignup End -->

        <div class="navbar navbar-vpshop navbar-fixed-top megamenu" role="navigation">
