                </div>

                <!-- this part is duplicate from cartMenu  keep it for mobile -->
                <div class="navbar-cart collapse">
                    <div class="cartMenu  col-lg-4 col-xs-12 col-md-4" ng-show="subTotal > 0" ng-hide="subTotal <= 0">
                        <div class="miniCartTable scroll-pane mCustomScrollbar">
                            <table>
                                <tbody>
                                    <tr class="miniCartProduct" ng-repeat="myCart in myCartService.myCarts">
                                        <td style="20%" class="miniCartProductThumb"><div> <a href="" ng-click="helperService.moveToPage('productDetails', myCart.ItemId)"> <img src="{{ myCart.ImgUrl }}" alt="img"> </a> </div><label>{{ myCart.ImgUrl }}</label></td>
