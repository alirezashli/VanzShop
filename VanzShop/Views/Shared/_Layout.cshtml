                                                <td style="width:40%">
                                                    <div class="miniCartDescription">
                                                        <h4> <a href="" ng-click="helperService.moveToPage('productDetails', myCart.ItemId)"> {{ myCart.Description }} </a> </h4>
                                                        <div class="price"> <span> ${{ myCart.PriceSale }} </span> </div>
                                                    </div>
                                                </td>
                                                <td style="width:10%" class="miniCartQuantity"><a> X {{ myCart.Qty }} </a></td>
                                                <td style="width:15%" class="miniCartSubtotal"><span> ${{ myCart.PriceSale * myCart.Qty }} </span></td>
                                                <td style="width:5%" class="delete"><a ng-click="myCartService.removeFromCart(myCart)"> x </a></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
