                                            <div class="miniCartDescription">
                                                <h4> <a href="" ng-click="helperService.moveToPage('productDetails', myCart.ItemId)"> {{ myCart.Description }} </a> </h4>
                                                <div class="price"> <span> ${{ myCart.PriceSale }} </span> </div>
                                            </div>
                                        </td>
                                        <td style="10%" class="miniCartQuantity"><a> X {{ myCart.Qty }} </a></td>
                                        <td style="15%" class="miniCartSubtotal"><span> ${{ myCart.PriceSale * myCart.Qty }} </span></td>
                                        <td style="5%" class="delete" ng-click="myCartService.removeFromCart(myCart)"><a> x </a></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <!--/.miniCartTable-->

                        <div class="miniCartFooter  miniCartFooterInMobile text-right">
                            <h3 class="text-right subtotal"> Subtotal: ${{ subTotal }} </h3>
                            <a class="btn btn-sm btn-danger" ng-click="helperService.moveToPage('cart')"> <i class="fa fa-shopping-cart"> </i> VIEW CART </a> <a class="btn btn-sm btn-primary" ng-click="checkout()"> CHECKOUT </a>
